{% extends 'base.html.twig' %}

{% block body %}
    <div class="img-head">
        <img src="{{ asset('images/mission.png') }}" class="img-responsive" />
    </div>

    <div class="container main-box">
        <div class="row">
        <div class="col-sm-12 main-cell">
            <h2>{{ post.title }} - {{ post.date | localizeddate('full', 'none', app.request.locale) | title }}</h2>
            {{ post.text | raw }}
        </div>
        </div>
    </div>
{% endblock %}
