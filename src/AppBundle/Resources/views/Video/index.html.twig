{% extends "::base.html.twig" %}

{% block title %}{{ parent() }} - Video{% endblock %}

{% block body %}
    <div class="img-head">
        <img src="{{ asset('images/mission.png') }}" class="img-responsive" />
    </div>

    <div class="container main-box">
        <div class="row">
            <div class="col-sm-12 main-cell">
                {% for video in videos %}
                    <div class="video embed-responsive embed-responsive-16by9">    
                        <iframe class="embed-responsive-item" src="{{ video.url }}" frameborder="0" allowfullscreen></iframe>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}
